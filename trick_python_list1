def tuples_to_list_string(lst):
    result = list(map(' '.join, lst))
    return result
collors = [('red', 'pink'),('white', 'black'),('orange', 'green')]
print("Original list of tuples:")
print(collors)
print("\nConvert the said list of tuples to list of stringsL:")
print(tuples_to_list_string(collors))